    "@types/mocha": "^7.0.2",
    "@types/node": "^13.11.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "codecov": "^3.6.5",
    "mocha": "^7.1.1",
    "nock": "^12.0.3",
    "nyc": "^15.0.1",
    "source-map-support": "^0.5.16",
    "ts-node": "^8.8.1",
